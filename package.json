    "createdevices": "node ./createdevices.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/CalcProgrammer1/OpenRGB-Website.git"
  },
  "keywords": [
    "openrgb",
    "rgb",
    "pc",
    "linux",
    "windows"
  ],
  "author": "CalcProgrammer1",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/CalcProgrammer1/OpenRGB-Website/issues"
  },
  "homepage": "https://gitlab.com/CalcProgrammer1/OpenRGB-Website#readme",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@parcel/packager-raw-url": "^2.8.0",
    "@parcel/packager-xml": "^2.8.0",
    "@parcel/transformer-pug": "^2.8.0",
    "@parcel/transformer-stylus": "^2.8.0",
    "@parcel/transformer-webmanifest": "^2.8.0",
    "@parcel/transformer-xml": "^2.8.0",
    "jquery": "^3.6.1",
    "jquery.scrollex": "^0.2.1",
    "marked": "^4.2.3",
    "parcel": "^2.8.0",
    "parcel-resolver-ignore": "^2.1.3",
